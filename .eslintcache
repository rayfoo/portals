["142"],"import React from 'react';\n\nimport { Title, Body, Minion } from '../Elements/Text';\nimport { Avatar } from '../Elements/Avatar';\nimport { PostBubble } from '../PostBubble';\nimport { PostType } from '../Post';\n\ntype PostProps = {\n  post: PostType;\n};\n\ntype ContentProps = {\n  header: React.ReactNode;\n  children: React.ReactNode;\n  post: PostType;\n};\n\nexport function Reply({ post }: PostProps) {\n  const isExpandable = post.body.length > 280;\n\n  const [expandState, setExpandState] = React.useState(!isExpandable);\n\n  const toggleBodyExpand = () => setExpandState((previous) => !previous);\n\n  const Header = (\n    <>\n      <Title styles=\"inline text-sm\">{post.user.handle}</Title>\n    </>\n  );\n\n  const Expandable = (\n    <div onClick={toggleBodyExpand}>\n      {expandState ? (\n        <Minion clickable>Show less</Minion>\n      ) : (\n        <Minion clickable styles=\"text-blue-600\">\n          Show more\n        </Minion>\n      )}\n    </div>\n  );\n\n  return (\n    <div className=\"flex flex-row mt-3\">\n      <Avatar url={post.user.avatarURL} size=\"sm\" alt=\"user avatar\" />\n\n      <Content header={Header} post={post}>\n        <PostBubble>\n          <Body clickable>\n            {expandState ? post.body : post.body.slice(0, 279)}\n          </Body>\n\n          {isExpandable && Expandable}\n        </PostBubble>\n      </Content>\n    </div>\n  );\n}\n\nfunction Replies({ count }: { count: number }) {\n  const NoReplies = (\n    <Minion clickable styles=\"ml-auto mt-1 pr-2\">\n      Reply\n    </Minion>\n  );\n\n  const HasReplies = (\n    <div\n      className={`ml-6 -mt-3 pt-3 pb-1 px-2 bg-blue-600 rounded-b-2xl -z-10`}\n    >\n      <Minion clickable invert styles=\"float-right\">\n        {count} replies\n      </Minion>\n    </div>\n  );\n\n  return <>{count ? HasReplies : NoReplies}</>;\n}\n\nfunction Content({ header, post, children }: ContentProps) {\n  return (\n    <div className=\"ml-2 flex flex-col flex-grow\">\n      <div className=\"mb-1 md:mb-2\">{header}</div>\n\n      {children}\n    </div>\n  );\n}\n","/Users/RXF1129/workspace/projects/portals/src/components/utils.ts",[],"/Users/RXF1129/workspace/projects/portals/src/features/ReplyContainer/index.tsx",[],"/Users/RXF1129/workspace/projects/portals/src/features/ReplyContainer/ReplyContainer.tsx",[],"/Users/RXF1129/workspace/projects/portals/src/store/index.tsx",[],"/Users/RXF1129/workspace/projects/portals/src/store/store.tsx",[],"/Users/RXF1129/workspace/projects/portals/src/store/reducer.tsx",[],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","severity":1,"message":"148","line":60,"column":10,"nodeType":"149","messageId":"150","endLine":60,"endColumn":17},"no-native-reassign",["151"],"no-negated-in-lhs",["152"],"@typescript-eslint/no-unused-vars","'Replies' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]